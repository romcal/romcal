{
  "name": "romcal",
  "version": "1.3.1-canary.39",
  "description": "Utility library that outputs the Liturgical Calendar used by the Roman Rite (Western Church)",
  "main": "dist/index.js",
  "module": "src/index.js",
  "engines": {
    "node": ">=8"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run flow && npm run build:babel && npm run build:webpack",
    "build:babel": "babel src --out-dir dist",
    "build:webpack": "webpack-cli --config webpack.config.js",
    "pretest": "./node_modules/.bin/eslint --ignore-path .gitignore ./src --fix",
    "flow": "flow",
    "test": "mocha 'test/*.js'",
    "updateChangelog": "auto-changelog -p"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint — fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romcal/romcal.git"
  },
  "keywords": [
    "roman",
    "catholic",
    "calendar",
    "latin",
    "western",
    "church",
    "christian",
    "seasons",
    "liturgical"
  ],
  "author": {
    "name": "Julian Pereira",
    "email": "bleushade@gmail.com",
    "url": "https://github.com/pejulian"
  },
  "contributors": [
    {
      "name": "Etienne Magnier",
      "email": "etienne.magnier@gmail.com",
      "url": "https://github.com/emagnier"
    },
    {
      "name": "Jacek Roszkowski",
      "url": "https://github.com/jarosz"
    },
    {
      "name": "Tukusej’s Sirs",
      "email": "tukusejssirs@protonmail.com",
      "url": "https://github.com/tukusejssirs"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romcal/romcal/issues"
  },
  "homepage": "https://github.com/romcal/romcal/blob/master/README.md",
  "dependencies": {
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-range": "^4.0.2",
    "moment-recur": "^1.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-flow": "^7.8.3",
    "@babel/register": "^7.8.3",
    "auto-changelog": "^1.16.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-node": "^11.0.0",
    "flow-bin": "^0.117.0",
    "lint-staged": "^10.0.7",
    "mocha": "^6.2.2",
    "semver": "^7.1.1",
    "should": "^13.2.3",
    "source-map-loader": "^0.2.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
